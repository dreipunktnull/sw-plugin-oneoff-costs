<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="dpn_oneoff_costs.subscriber.extend_add_article" class="DpnOneoffCosts\Subscriber\ExtendAddArticleSubscriber">
            <argument type="service" id="service_container"/>
            <tag name="shopware.event_subscriber"/>
        </service>
        <service id="dpn_oneoff_costs.subscriber.delete_article" class="DpnOneoffCosts\Subscriber\DeleteArticleSubscriber">
            <argument type="service" id="service_container"/>
            <tag name="shopware.event_subscriber"/>
        </service>
    </services>

</container>
